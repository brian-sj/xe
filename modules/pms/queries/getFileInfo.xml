<query id="getFileInfo" action="select">
    <tables>
        <table name="pms_files" />
    </tables>
    <columns>
        <column name="*" />
    </columns>	
    <conditions>
        <condition operation="equal" column="pms_file_srl" var="pms_file_srl" filter="number" notnull="notnull" />
    </conditions>
</query>
