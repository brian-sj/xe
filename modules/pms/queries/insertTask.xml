<query id="insertTask" action="insert">
    <tables>
        <table name="pms_task" />
    </tables>
    <columns>
		<column name="project_srl" var="project_srl" notnull="notnull"/>
		<column name="task_srl" var="task_srl" notnull="notnull" />
		<column name="parent_task_srl" var="parent_task_srl" default="0"/>
		<column name="member_srl" var="member_srl" />					
		<column name="work_member_srl" var="work_member_srl" />					
		<column name="progress" var="progress" default="0" />				
		<column name="stt" var="stt" default="N" />				
		<column name="mh" var="mh" default="0" />				
		<column name="planed_mh" var="planed_mh" default="0" /> 
		<column name="title" var="title" default="_" /> 
		<column name="memo" var="memo" />					
		<column name="start_date" var="start_date" />
		<column name="end_date" var="end_date" /> 
		<column name="planed_start_date" var="planed_start_date" /> 
		<column name="planed_end_date" var="planed_end_date" /> 
		<column name="reg_date" default="curdate()" />

		<column name="code_urgency" var="code_urgency" />					
		<column name="code_importance" var="code_importance" />					
		<column name="sort_order" var="sort_order" />					
		<column name="ext_vars" var="ext_vars" />	
			
    </columns>
</query>	