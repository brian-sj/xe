<filter name="bookc_insert" module="book" act="procBookcWrite2" confirm_msg_code="confirm_submit">
    <form>
        <node target="mid" required="false" maxlength="40" filter="alpha_number" />
        <node target="title" required="true" maxlength="250" />
    </form>
	<response callback_func="completeBookcWrite2">
        <tag name="error" />
        <tag name="message" />
        <tag name="module" />
        <tag name="act" />
        <tag name="page" />
        <tag name="module_srl" />
    </response>
</filter>
